   -
     name : _DirectoryModifications
     internal_properties : CAAAAAgAAADk1dQIZps3V5AzhY0LcwHByKefUDfyjzs1y1F/nTT18nTXdGIIRzESoBUVghcbPz4GgLry6ezFtojQiZ10/QRkAIAVU027efDWO4FE33xwBPhJDyNqAfB0zL0zvDiJv84kCs5nGxQDrZ0zPbV3vvle9ef4zvtsftlcl1w2VzA=
     procedure_id : 1234599262751603547
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntaxe :
      //_DirectoryModifications (<sDirectoryName> est chaîne, <sFileName> est chaîne, <nTypeOfChange> est entier, <sOldName>)
      //
      // Paramètres :
      //	sDirectoryName (chaîne ANSI) : <specify the role of sElement>
      //	sFileName (chaîne ANSI) : <specify the role of sParameter>
      //	nTypeOfChange (entier) : <specify the role of nTypeOfChange>
      //	sOldName : <specify the role of sParameter>
      // Example:
      // Indicate an example.
      //
      // Return value:
      Procedure _DirectoryModifications(sDirectoryName is string, sFileName is string, nTypeOfChange is int, sOldName)
      
      sMessage is string
      sElement is string = sDirectoryName + [“\”] + sFileName
      
      SWITCH nTypeOfChange
      	CASE ftDeleteFile
      		sMessage = gImage(IMG_DEL) + “ ” + StringBuild(“The <%1> element (file or directory) was deleted”, sElement)
      		
      	CASE ftCreateFile		
      		// Check the existence of the file, if it does not exist, it's a directory
      		IF fFileExist(sElement) = False THEN
      			// It's a directory
      			sMessage = gImage(IMG_ADD) + “ ” + StringBuild(“The <%1> directory was added”, sElement)	
      		ELSE
      			sMessage = gImage(IMG_ADD) + “ ” + StringBuild(“The <%1> file was added”, sElement)
      		END
      	CASE ftRename
      		// Check the existence of the file, if it does not exist, it's a directory
      		IF fFileExist(sElement) = False THEN
      			// It's a directory
      			sMessage = gImage(IMG_RENAME) + “ ” + StringBuild(“The <%1> directory was renamed to <%2>”, sOldName, sElement)
      		ELSE
      			
      			sMessage = gImage(IMG_RENAME) + “ ” + StringBuild(“The <%1> file was renamed to <%2>”, sOldName, sElement)
      		END
      		
      	CASE ftModifyFile
      		// Check the existence of the file, if it does not exist, it's a directory
      		IF fFileExist(sElement) = False THEN
      			// It's a directory
      			sMessage = gImage(IMG_MODIF) + “ ” + StringBuild(“The <%1> directory was modified”, sElement)	
      		ELSE
      			sMessage = gImage(IMG_MODIF) + “ ” + StringBuild(“The <%1> file was modified”, sElement)
      		END
      		
      	OTHER CASE
      		dbgAssert(False,“Unknown notification”)
      		RETURN
      END
      
      
      AddLog(sMessage)
